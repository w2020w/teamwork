import pandas as pd
df=pd.read_excel('/Users/wangjia/Desktop/haerbinggongye.xlsx')
height,width = df.shape
print(height,width,type(df))
print('排序之前')
print(df)
print('排序之后')
print(df.sort_values('参与人数', ascending= False))
print('程序顺利执行完成')





